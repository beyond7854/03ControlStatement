package ex04controlstatement;

public class dandan
{

	public static void main(String[] args)
	{

		int max =9;
		int dan = 2;
		
		while(dan<=max){
			int su = 1;
				while(max >= su) {
					System.out.print(dan+ "*" + su +"="+ (dan*su));
					
					System.out.print(" ");
					su++;
				}
				System.out.println(" ");
			
				dan ++;
			
		} 
	}

}
